{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore",
    "android": "cd apps/native && npm run android",
    "ios": "cd apps/native && npm run ios"
  },
  "devDependencies": {
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/web": " ^0.0.137",
    "cypress": "^13.6.4",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "react-test-renderer": "^18.2.0",
    "ts-jest": "^29.1.2",
    "turbo": "^1.11.3"
  },
  "packageManager": "npm@10.4.0",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "firebase": "^10.7.1",
    "postcss": "^8.4.33",
    "react-bootstrap": "^2.10.0",
    "react-firebase-hooks": "^5.1.1",
    "tailwindcss": "^3.4.1",
    "uuid": "^9.0.1",
    "expo-image-picker": "~14.3.2"
  }
}
